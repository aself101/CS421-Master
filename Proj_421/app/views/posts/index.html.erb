<h1>Listing Articulation</h1>
<%= link_to 'New Entry', new_post_path %>
<table>
  <tr>
    <th>University</th>
    <th>Department</th>
    <th>Course</th>
    <th>Year</th>
    <th>Semester</th>
  </tr>

  <% @posts.each do |post| %>
      <tr>
        <td><%= post.university %></td>
        <td><%= post.department %></td>
        <td><%= post.course %></td>
        <td><%= post.year %></td>
        <td><%= post.semester %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post_path(post),
                        method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
</table>