<h1>Student Submissions Page</h1>
<%= form_for :submission, url: submissions_path do |f| %>
    <p>
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </p>

    <p>
      <%= f.label :studentid %><br>
      <%= f.text_field :studentid %>
    </p>

    <p>
      <%= f.label :term %><br>
      <%= f.text_field :term %>
    </p>
    <p>
      <%= f.label :major %><br>
      <%= f.text_field :major %>
    </p>
    <p>
      <%= f.label :email %><br>
      <%= f.text_field :email %>
    </p>
    <p>
      <%= f.label :phone %><br>
      <%= f.text_field :phone %>
    </p>
    <p>
      <%= f.label :signature %><br>
      <%= f.text_field :signature %>
    </p>
    <p>
      <%= f.label :date %><br>
      <%= f.text_field :date %>
    </p>
    <p>
      <%= f.submit :submit %>
    </p>
<% end %>